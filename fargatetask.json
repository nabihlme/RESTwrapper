{
    "family": "wrapper", 
    "networkMode": "awsvpc", 
    "containerDefinitions": [
        {
            "name": "wrapper-task", 
            "image": "arn:aws:ecr:us-east-1:280922329489:repository/wrapper", 
            "portMappings": [
                {
                    "containerPort": 8000, 
                    "hostPort": 8000, 
                    "protocol": "tcp"
                }
            ], 
            "essential": true
        }
    ], 
    "requiresCompatibilities": [
        "FARGATE"
    ], 
    "cpu": "256", 
    "memory": "512"
}
